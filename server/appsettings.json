{
  "AccountStorePath": "Data/account-store.json",
  "DataProtection": {
    "KeysPath": "Data/keys",
    "KeyLifetimeDays": 90
  },
  "AllowedOrigins": [
    "http://localhost:5173",
    "http://127.0.0.1:5173"
  ],
  "GoogleOAuth": {
    "ClientId": "<set in env>",
    "ClientSecret": "<set in env>",
    "RedirectUri": "http://localhost:5000/api/oauth/google/callback"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
